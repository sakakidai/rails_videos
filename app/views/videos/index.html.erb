<p style="color: green"><%= notice %></p>

<h1>Videos</h1>
<div>
  <%= image_tag 'computer-2982270_1920.jpg' %>
</div>

<div id="videos">
  <% @videos.each do |video| %>
    <%= render video %>
    <p>
      <%= link_to "Show this video", video %>
    </p>
  <% end %>
</div>

<%= link_to "New video", new_video_path %>
